#JAVA aliases
alias java-17="export JAVA_HOME=`/usr/libexec/java_home -v 17`; java --version"
alias java-19="export JAVA_HOME=`/usr/libexec/java_home -v 19`; java --version"
export JAVA_HOME=`/usr/libexec/java_home -v 17`

# CEDAR install suggestion
export PATH=$(brew --prefix)/opt/openssl@1.1/bin:$PATH
export PATH="$(brew --prefix)/opt/node@16/bin:$PATH"
export PATH="$(brew --prefix)/anaconda3/bin/:$PATH"

# CEDAR development related scripts, aliases, environment variables
export CEDAR_HOME=${HOME}/CEDAR
alias cedarcli='source $CEDAR_HOME/cedar-cli/cli.sh'
source ${CEDAR_HOME}/cedar-profile-native-develop.sh

